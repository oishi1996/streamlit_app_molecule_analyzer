    "import joblib\n",
    "import pickle\n",
    "\n",
    "# もともとのpklをロード（初回だけ）\n",
    "with open(\"models/model.pkl\", \"rb\") as f:\n",
    "    model = pickle.load(f)\n",
    "\n",
    "# joblibで圧縮して保存（圧縮レベル3〜9が推奨）\n",
    "dump(model, \"models/model_compressed.joblib\", compress=3)\n"
